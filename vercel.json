{
  "rewrites": [
    {
      "source": "/.well-known/assetlinks.json",
      "destination": "/.well-known/assetlinks.json"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "v0-mwt-assistant.vercel.app"
        }
      ],
      "destination": "https://mwtassistant.com/:path*",
      "permanent": true
    }
  ]
}
